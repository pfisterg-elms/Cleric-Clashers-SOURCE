[sub_resource type="Resource" id="Resource_ol2ar"]
script = ExtResource("1_1gwo1")
narrator_title = ""
dialogue_contents = "The atrium left behind with the felled bodies of brigands, the cleric follows close with the knight, rounding the bend to the quarters of the temple. Sunlight glistens through the ruined structure, ashes and cinder rising in the air. An unbearable stench of death and destruction wafts through as the two march on."
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_l5pyo"]
script = ExtResource("1_1gwo1")
narrator_title = "The Knight"
dialogue_contents = "Keep moving. They may yet burn this sanctum until there is nothing but ash in its wake."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_seqah"]
script = ExtResource("1_1gwo1")
narrator_title = "The Cleric"
dialogue_contents = "But what of the others still within? We cannot be the only ones left amid the chaos."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_rfsxo"]
script = ExtResource("1_1gwo1")
narrator_title = ""
dialogue_contents = "The Knight is silent for a moment as they walk forth. His body heaves as he sighs with a shake of his head."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_ujq5o"]
script = ExtResource("1_1gwo1")
narrator_title = "The Knight"
dialogue_contents = "There is no one else. We are the last burning embers of light within this darkness. We shall not let them extinguish our flame."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_aoacs"]
script = ExtResource("1_1gwo1")
narrator_title = "The Cleric"
dialogue_contents = "..I see.

Then let us move with haste."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_48n6p"]
script = ExtResource("1_1gwo1")
narrator_title = "The Knight"
dialogue_contents = "Indeed. Follow my lead. I see two more ahead. Act swiftly by my side and we will proceed unscathed."
left_sprite = ExtResource("2_b0ygs")
right_sprite = ExtResource("3_1iq4g")
sprite_focus = "Right"

