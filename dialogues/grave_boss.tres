[sub_resource type="Resource" id="Resource_3eqwn"]
script = ExtResource("3_1ov70")
narrator_title = ""
dialogue_contents = "Undead skeletons torn asunder litter the graveyard. The gnarly battles with the necrotic fiends has been a blur and a frenzy, but the doctor continues to trudge forward, pointing at the tomb just ahead."
left_sprite = ExtResource("1_cpswk")
right_sprite = ExtResource("2_bhylr")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_fgk0q"]
script = ExtResource("3_1ov70")
narrator_title = "Doctor"
dialogue_contents = "Here we are, at last! Wait one moment and I shall open the passage for us. The city will soon be upon us, have no fear!"
left_sprite = ExtResource("1_cpswk")
right_sprite = ExtResource("2_bhylr")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_52hnm"]
script = ExtResource("3_1ov70")
narrator_title = ""
dialogue_contents = "The doctor steps forward, passing by two towering statues of gargoyles resting at the sides of the tomb. Beady red, glowing eyes haunt the gaze of the doctor.

...They have the feeling that they're being watched."
left_sprite = ExtResource("4_wqg4b")
right_sprite = ExtResource("5_ef11h")
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_mn2bp"]
script = ExtResource("3_1ov70")
narrator_title = ""
dialogue_contents = "Suddenly, with a crackle of stone and cement, the statue comes to life. Its eyes shimmer as its jaw extends, its claws jutting out from its rocky body. Its wings sprout, its body twisting to face the trio as the doctor steps back in cautious retreat."
left_sprite = ExtResource("4_wqg4b")
right_sprite = ExtResource("6_ga7d7")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_qb372"]
script = ExtResource("3_1ov70")
narrator_title = "Guardian Gargoyle"
dialogue_contents = "INTRUDERS. YOU DARE TRESPASS UPON THE DEAD. WHAT SAY YOU OF THIS HEINOUS CRIME."
left_sprite = ExtResource("7_eidjg")
right_sprite = ExtResource("6_ga7d7")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_ksuyj"]
script = ExtResource("3_1ov70")
narrator_title = "Doctor"
dialogue_contents = "I- Well, you must understand! That was simply an act of defense! We trespass, yes, but we must get through-"
left_sprite = ExtResource("7_eidjg")
right_sprite = ExtResource("6_ga7d7")
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_xim5i"]
script = ExtResource("3_1ov70")
narrator_title = "Guardian Gargoyle"
dialogue_contents = "YOU ADMIT GUILT. YOU WILL BE PUNISHED. THE DEAD CANNOT SPEAK FOR THEMSELVES. I WILL SPEAK FOR THEM.

I AM JUDGE. JURY. EXECUTIONER. AND SO YOUR PUNISHMENT... IS DEATH!"
left_sprite = ExtResource("7_eidjg")
right_sprite = ExtResource("6_ga7d7")
