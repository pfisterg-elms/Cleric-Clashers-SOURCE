[sub_resource type="Resource" id="Resource_k4bl0"]
script = ExtResource("1_inb1h")
narrator_title = ""
dialogue_contents = "The fog falters as the spirituals vanish into ash against the wind. A graveyard presents itself just down the path. The doctor eagerly approaches its front gate."
left_sprite = ExtResource("1_shnn5")
right_sprite = ExtResource("2_aycd3")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_psovq"]
script = ExtResource("1_inb1h")
narrator_title = "Doctor"
dialogue_contents = "Let us not idle! I've had my fill with the Celestials for one day. And what better safe haven than the fields of buried dead? Hurry along now, my new friends!"
left_sprite = ExtResource("1_shnn5")
right_sprite = ExtResource("4_8ufsu")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_0s8qj"]
script = ExtResource("1_inb1h")
narrator_title = ""
dialogue_contents = "Henrick and Claira look toward each other, before begrudgingly following the doctor as they enthusiastically dash into the graveyard. Clearly enjoying this exercise of violence and chaos more than the others."
left_sprite = ExtResource("1_shnn5")
right_sprite = ExtResource("5_h6yq5")
sprite_focus = "Left"

[sub_resource type="Resource" id="Resource_7p4u0"]
script = ExtResource("1_inb1h")
narrator_title = "Doctor"
dialogue_contents = "I see it both within you, you wish to seek Creitus like I do. Worry not, I know of a shortcut through the graveyard. An underground tomb acts as passageway to its outskirts. It won't be long now!"
left_sprite = ExtResource("1_shnn5")
right_sprite = ExtResource("4_8ufsu")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_akjx7"]
script = ExtResource("1_inb1h")
narrator_title = ""
dialogue_contents = "As the doctor's voice hushes, mumbles and groans echo from the very earth itself. Grave mounds shake as skeletal hands claw through to break out onto the surface. A sickly green mist shrouds the graveyard as the air is filled with an aroma of death and decay."
left_sprite = ExtResource("6_wgyyt")
right_sprite = ExtResource("7_qa6l7")
sprite_focus = "Right"

[sub_resource type="Resource" id="Resource_clhd8"]
script = ExtResource("1_inb1h")
narrator_title = "Doctor"
dialogue_contents = "...Ah, of course. How unwise of me to forget that this graveyard is as accursed as the forest!

Very well, no matter. Follow my lead, you two. We shall have to dance with the dead to yet survive this fright! Forward unto the fray, for what better laboratory than a battlefield?"
left_sprite = ExtResource("8_45rxr")
right_sprite = ExtResource("9_3b4rt")
sprite_focus = "Left"

